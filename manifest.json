{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "ResearchSEO",
  "short_name": "terumit",
  "description": "SEOに関するタグを検知、参照するツール",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "terumit",
    "default_popup": "popup.html"
  },
  "icons" : {
     "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contents.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ]
}
